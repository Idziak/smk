{
  "name": "SMK importer",
  "description": "Automatyczny import wykonanych zabieg√≥w i procedur do aplikacji SMK.",
  "version": "1.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "settings.html"
  },
  "permissions": [
    "scripting", "storage"
  ],
  "host_permissions": [ "http://smk.ezdrowie.gov.pl/*", "https://smk.ezdrowie.gov.pl/*"],
  "content_scripts": [
    {
      "js": [ "jquery-3.6.0.min.js" ],
      "matches": [ "http://smk.ezdrowie.gov.pl/*", "https://smk.ezdrowie.gov.pl/*"]
    }
  ]
}